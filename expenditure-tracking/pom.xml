<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>org.fenixedu</groupId>
        <artifactId>fenix-framework-project</artifactId>
        <version>2.5.2</version>
        <relativePath />
    </parent>

    <artifactId>expenditure-tracking</artifactId>
    <name>Acquisitions and Mission Module</name>
    <version>9.1.0-SNAPSHOT</version>
    <packaging>jar</packaging>

    <properties>
        <jasper.threads>1</jasper.threads>
        <version.org.fenixedu.bennu.core>6.1.0</version.org.fenixedu.bennu.core>
        <version.org.fenixedu.workflow>4.0.0</version.org.fenixedu.workflow>
        <version.org.fenixedu.organization>4.0.0</version.org.fenixedu.organization>
        <version.org.fenixedu.dashboard>3.0.0</version.org.fenixedu.dashboard>
        <version.org.fenixedu.db.utils>2.1.0</version.org.fenixedu.db.utils>
        <version.org.fenixedu.messaging>4.0.0</version.org.fenixedu.messaging>
        <version.org.fenixedu.bennu-renderers>6.0.1</version.org.fenixedu.bennu-renderers>
        <version.org.fenixedu.commons>3.0.0</version.org.fenixedu.commons>
        <version.jfree.jfreechart>0.9.18</version.jfree.jfreechart>
        <version.net.sf.jasperreports>3.6.0</version.net.sf.jasperreports>
        <version.org.glassfish.jersey>2.27</version.org.glassfish.jersey>
        <version.pt.ist.fenix.legacy.tools>2.1.0</version.pt.ist.fenix.legacy.tools>
        <version.org.webjars.bootstrap-datetimepicker>2.4.2</version.org.webjars.bootstrap-datetimepicker>
        <version.pt.ist.papyrus-client>2.0.0</version.pt.ist.papyrus-client>
        <version.com.itextpdf>5.5.13</version.com.itextpdf>
    </properties>

    <build>
        <plugins>
            <plugin>
                <groupId>com.alexnederlof</groupId>
                <artifactId>jasperreports-plugin</artifactId>
                <version>1.5.1</version>
                <executions>
                    <execution>
                        <phase>compile</phase>
                        <goals>
                            <goal>jasper</goal>
                        </goals>
                    </execution>
                </executions>
                <dependencies>
                    <dependency>
                        <groupId>net.sf.jasperreports</groupId>
                        <artifactId>jasperreports</artifactId>
                        <version>${version.net.sf.jasperreports}</version>
                    </dependency>
                    <dependency>
                        <groupId>log4j</groupId>
                        <artifactId>log4j</artifactId>
                        <version>1.2.16</version>
                    </dependency>
                </dependencies>
                <configuration>
                    <compiler>pt.ist.expenditureTrackingSystem.util.FenixJasperCompiler</compiler>
                    <outputDirectory>${project.build.outputDirectory}/reports</outputDirectory>
                    <numberOfThreads>${jasper.threads}</numberOfThreads>
                </configuration>
            </plugin>
        </plugins>
    </build>

    <dependencies>
		<!-- iText is needed to generate QRCode and signature field -->
		<dependency>
