			<artifactId>itextpdf</artifactId>
			<version>${version.com.itextpdf}</version>
		</dependency>
		<!-- Papyrus SDK for generation of PDF documents using html templates and JSON data -->
	    <dependency>
			<groupId>pt.ist</groupId>
			<artifactId>papyrus-client</artifactId>
			<version>${version.pt.ist.papyrus-client}</version>
	    </dependency>
        <dependency>
